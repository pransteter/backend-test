{
  "name": "best-travel-quotation",
  "version": "1.0.0",
  "description": "Travel Price Quotation is a simple app to quote wich is the best price to travel from an airport to another.",
  "main": "./src/api/index.js",
  "scripts": {
    "start-api": "node ./src/api/index.js",
    "test": "jest --colors",
    "get-best-travel-quotation": "node ./src/scripts/get-best-travel-quotation.js"
  },
  "author": "Romulo Pransteter da Silva",
  "license": "SEE LICENSE IN LICENSE.txt",
  "type": "module",
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?)$",
    "collectCoverage": true,
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/src/api/**"
    ],
    "transform": {
      ".(js)": "@sucrase/jest-plugin"
    }
  }
}
